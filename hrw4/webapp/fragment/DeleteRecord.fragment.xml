	<VBox>
		<f:Form id="FormDisplay354wideDual" editable="false">
			<f:layout>
				<f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="3" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="4"
					emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="1" columnsM="1" singleContainerFullSize="false"/>
			</f:layout>
			<f:formContainers>
				<f:FormContainer title="{i18n>W4WText}">
					<f:formElements>
						<f:FormElement label="{i18n>TaxText}">
							<f:fields>
								<Text text="{SupplierName}" id="nameText"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="{i18n>ExemptionsText}">
							<f:fields>
								<Text text="{Street} {HouseNumber}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="{i18n>AdditionalText}">
							<f:fields>
								<Text text="{ZIPCode} {City}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="{i18n>TaxExText}">
							<f:fields>
								<Text text="{Country}" id="countryText"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="{i18n>StatusText}" class="sapUiLargeMarginTop">
							<f:fields>
								<CheckBox></CheckBox>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="{i18n>Status1Text}" class="sapUiLargeMarginTop">
							<f:fields>
								<CheckBox></CheckBox>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="{i18n>Check1Text}" class="sapUiLargeMarginTop">
							<f:fields>
								<HBox>
									<CheckBox select="onCheckChange"></CheckBox>
									<MessageStrip id="idMessageCall" type="Success" visible="false" text="{i18n>Check2Text}" showIcon="false" showCloseButton="false"></MessageStrip>
								</HBox>
							</f:fields>
