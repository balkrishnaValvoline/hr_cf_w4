<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:unified="sap.ui.unified">
	<HBox width="100%" id="logButtonContainer" class="logButtonFixHeight logButtonFixHeight_Impersonate huserinfo-frag-container"
		justifyContent="SpaceBetween">
		<VBox>
			<HBox>
				<Text id="idUserFName" class="customIcon customIcon--header help-button headerUserF"></Text>
				<Text id="idUserLName" class="customIcon customIcon--header help-button headerUserL"></Text>
			</HBox>
			<HBox>
				<Button icon="sap-icon://customer" iconFirst="false" id="logButton" class="customIcon customIcon--header logoutButton headerLogin"
					text="{i18n>LogoutBTN}" press="logoutPress"></Button>
			</HBox>
		</VBox>
	</HBox>
</core:FragmentDefinition>