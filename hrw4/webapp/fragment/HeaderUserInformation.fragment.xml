<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:unified="sap.ui.unified">
	<HBox width="100%" id="logButtonContainer" class="logButtonFixHeight logButtonFixHeight_Impersonate huserinfo-frag-container"
		justifyContent="SpaceBetween">
		<HBox>
			<Button icon="sap-icon://sys-help" iconFirst="false" id="helpButton" class="customIcon customIcon--header help-button"
				text="{i18n>HelpBTN}" press="toHelp"></Button>
			<Button icon="sap-icon://customer"
				visible="{parts: ['oAccounts>/firstname', 'oModelImpersonate>/impersonateStatus'], formatter : '.checkImpersonate'}" iconFirst="false"
				id="logButton" class="customIcon customIcon--header logoutButton" text="{i18n>LogoutBTN}" press="logUserOut"></Button>
		</HBox>
	</HBox>
</core:FragmentDefinition>