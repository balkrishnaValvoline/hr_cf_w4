<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:unified="sap.ui.unified">
	<HBox width="100%" id="logButtonContainer" class="logButtonFixHeight logButtonFixHeight_Impersonate huserinfo-frag-container"
		justifyContent="SpaceBetween">
		<HBox>
			<HBox>
				<Button id="idUserFName" class="customIcon customIcon--header logoutButton headerLogin logoutButtonPressed"></Button>
				<Button icon="sap-icon://customer" iconFirst="false" id="idUserLName"
					class="customIcon customIcon--header logoutButton headerLogin headingNames logoutButtonPressed"></Button>
				<Button icon="sap-icon://initiative" iconFirst="false" id="logButton"
					class="customIcon customIcon--header logoutButton headerLogin logoutmargin logoutButtonPressed" text="{i18n>LogoutBTN}" press="logoutPress"></Button>
			</HBox>
		</HBox>
	</HBox>
</core:FragmentDefinition>